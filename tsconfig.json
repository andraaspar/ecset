{
	"compilerOptions": {
		"target": "es6",
		"module": "es6",
		"moduleResolution": "node",
		"lib": [
			"dom",
			"es6"
		],
		"baseUrl": ".",
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noImplicitReturns": true,
		"forceConsistentCasingInFileNames": true
		// "strictNullChecks": true
	},
	"exclude": [
		"bower_components",
		"node_modules"
	]
}