{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"moduleResolution": "node",
		"lib": [
			"dom",
			"es5",
			"es2015.promise"
		],
		"baseUrl": ".",
		"noImplicitThis": true,
		"noImplicitAny": true,
		"noImplicitReturns": true
	},
	"exclude": [
		"bower_components",
		"node_modules"
	]
}